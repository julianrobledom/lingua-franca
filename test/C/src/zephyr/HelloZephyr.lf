target C {
    build: "bash scripts/zephyr_build.sh  qemu_cortex_m3 flash",
    cmake-include: "scripts/zephyr.cmake",
    threading: false
}

main reactor {
    timer t (0, 500 msec)
    reaction(t) {=
        instant_t log = lf_time_logical_elapsed();
        instant_t phy = lf_time_physical_elapsed();
        printk("logical=%lli physical=%lli diff = %lli Hello World!\n", log, phy, phy-log);
    =}
}