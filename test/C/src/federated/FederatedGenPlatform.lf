/**
 * Tests if the federate emitter correctly handles an overloaded definition of the platform
 * parameter (i.e. without key-value pairs)
 */
target C {
  timeout: 2 secs,
  platform: "Linux"
}

reactor R {
  input in: int
  output out: int
}

federated reactor {
  r1 = new R()
  r2 = new R()
  r1.out -> r2.in
  r2.out -> r1.in
}
