/**
 * This simple test checks if federate authentication works by adding `auth`
 * target property.
 */
 target C {
    timeout: 2 secs,
    build-type: RelWithDebInfo,
    sst: true,
    logging: DEBUG
}

reactor Fed {
    input in: int
    output out: int
}

federated reactor {
    fed1 = new Fed()
}
